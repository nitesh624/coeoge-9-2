{
  "name": "iitbombayweb/iitbdeptwebproject",
  "description": "IIT Bombay Department Website Distribution Project",
  "type": "project",
  "license": "GPL-2.0-or-later",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.3.0",
    "drupal/core-recommended": "~9.2.0",
    "drupal/core-composer-scaffold": "~9.2",
    "szeidler/composer-patches-cli": "~1.0.0",
    "oomphinc/composer-installers-extender": "~2.0",
    "wikimedia/composer-merge-plugin": "~2.0",
    "drupal/console": "~1.0",
    "drush/drush": "^8.0",
    "solarium/solarium": "~6.1",
    "iitbombayweb/iitbdeptweb": "~9.2.0",
    "iitbombayweb/iitbcommon": "~9.2.0",
    "iitbombayweb/iitbextra": "~9.2.0",
    "iitbombayweb/acadcommon": "~9.2.0",
    "ckeditor/liststyle": "4.8.0",
    "ckeditor/tabletoolstoolbar": "^0.0.1",
    "ckeditor/tableselection": "^4.7",
    "colorbox/colorbox": "^1.0",
    "drupal/ckeditor_liststyle": "^1.4",
    "drupal/ckeditor_tabletoolstoolbar": "^1.0",
    "drupal/ckeditor_tableselection": "^1.0",
    "drupal/fvm": "^2.0",
    "drupal/upgrade_status": "^3.6",
    "drupal/color_field": "^2.4",
    "drupal/editor_advanced_link": "^1.9",
    "drupal/yearonly": "^9.0",
    "drupal/datetimehideseconds": "^1.1",
    "drupal/paragraphs_jquery_ui_accordion": "^1.4",
    "drupal/entity_usage": "^2.0@beta",
    "drupal/draggableviews": "^2.0",
    "drupal/transliterate_filenames": "^1.5",
    "drupal/flexslider": "^2.0",
    "drupal/views_contextual_filters_or": "^1.2",
    "drupal/quick_node_clone": "^1.14",
    "drupal/viewsreference": "^1.7",
    "drupal/entity_browser": "^2.6",
    "drupal/allowed_formats": "^1.3",
    "drupal/colorbox": "^1.7",
    "drupal/entity_browser_table": "^1.2",
    "drupal/inline_entity_form": "^1.0@RC",
    "drupal/media_entity_browser": "^2.0@alpha",
    "drupal/twig_tweak": "^2.9",
    "drupal/paragraphs": "^1.12",
    "drupal/ctools": "^3.7",
    "iitbombayweb/coeoge_iitb_dj": " dev-master",
    "bower-asset/flexslider": " ~2.7.2",
    "drupal/jquery_ui_accordion": "^1.1",
    "bower-asset/jquery-simple-color": " 1.1.5",
    "bower-asset/spectrum": " 1.8.1"
  },
  "conflict": {
    "symfony/http-foundation": "3.4.24"
  },
  "repositories": {
    "drupal": {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    },
    "asset": {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    "acadmixdist": {
      "type": "github",
      "url": "https://github.com/iitbombayweb/iitbdeptweb",
      "no-api": true
    },
    "iitbcommonmodules": {
      "type": "github",
      "url": "https://github.com/iitbombayweb/iitbcommon",
      "no-api": true
    },
    "iitbextramodules": {
      "type": "github",
      "url": "https://github.com/iitbombayweb/iitbextra",
      "no-api": true
    },
    "acadcommonmodules": {
      "type": "github",
      "url": "https://github.com/iitbombayweb/acadcommon",
      "no-api": true
    },
    "groupmenu": {
      "type": "package",
      "package": {
        "name": "drupal_git/groupmenu",
        "type": "drupal-module",
        "version": "1.0-beta2",
        "source": {
          "type": "git",
          "url": "https://git.drupalcode.org/project/groupmenu.git",
          "reference": "tags/8.x-1.0-beta2"
        }
      }
    },
    "coeoge_iitb_dj": {
      "type": "git",
      "url": "https://github.com/IITBombayWeb/coeoge_iitb_dj",
      "no-api": true
    },
    "ckeditor-tableselection": {
      "type": "package",
      "package": {
        "name": "ckeditor/tableselection",
        "version": "4.7.2",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/tableselection/releases/tableselection_4.7.2.zip",
          "type": "zip"
        }
      }
    },
    "ckeditor-tabletoolstoolbar": {
      "type": "package",
      "package": {
        "name": "ckeditor/tabletoolstoolbar",
        "version": "0.0.1",
        "type": "drupal-library",
        "dist": {
          "url": "https://download.ckeditor.com/tabletoolstoolbar/releases/tabletoolstoolbar_0.0.1.zip",
          "type": "zip"
        }
      }
    },
    "ckeditor-liststyle": {
      "type": "package",
      "package": {
        "name": "ckeditor/liststyle",
        "version": "4.8.0",
        "type": "drupal-library",
        "extra": {
          "installer-name": "ckeditor/plugins/liststyle"
        },
        "dist": {
          "url": "https://download.ckeditor.com/liststyle/releases/liststyle_4.8.0.zip",
          "type": "zip"
        }
      }
    },
    "colorbox": {
      "type": "package",
      "package": {
        "name": "colorbox/colorbox",
        "version": "1.0",
        "type": "drupal-library",
        "dist": {
          "url": "https://github.com/jackmoore/colorbox/archive/1.x.zip",
          "type": "zip"
        }
      }
    }

  },
  "config": {
    "process-timeout": 0
  },
  "extra": {
    "drupal-scaffold": {
      "locations": {
        "web-root": "docroot/"
      }
    },
    "merge-plugin": {
      "include": [
        "docroot/profiles/contrib/iitbdeptweb/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "merge-extra": true,
      "merge-extra-deep": true,
      "merge-scripts": true
    },
    "installer-types": [
      "bower-asset",
      "npm-asset"
    ],
    "installer-paths": {
      "docroot/core": [
        "type:drupal-core"
      ],
      "docroot/libraries/{$name}": [
        "type:drupal-library",
        "type:bower-asset",
        "type:npm-asset"
      ],
      "docroot/modules/contrib/{$name}": [
        "type:drupal-module"
      ],
      "docroot/profiles/contrib/{$name}": [
        "type:drupal-profile"
      ],
      "docroot/themes/contrib/{$name}": [
        "type:drupal-theme"
      ],
      "patch/{$name}": [
        "type:drupal-patch"
      ]
    },
    "enable-patching": true,
    "patches-file": "composer.patches.json"
  },
  "scripts": {
    "post-create-project-cmd": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
    "post-update-cmd": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold"
  }
}
